[run]
branch = True
include =
    **/.tox/*/lib/*/site-packages/hypothesis/*.py
    **/.tox/*/lib/*/site-packages/hypothesis/**/*.py
omit =
    **/pytestplugin.py
    **/strategytests.py
    **/compat*.py
    **/extra/__init__.py
    **/.tox/*/lib/*/site-packages/hypothesis/internal/coverage.py

[report]
exclude_lines =
    @abc.abstractmethod
    @abc.abstractproperty
    NotImplementedError
    pragma: no cover
    __repr__
    __ne__
    __copy__
    __deepcopy__
    except ImportError:
    if PY2:
    assert all\(.+\)
    if __reserved is not not_set:
